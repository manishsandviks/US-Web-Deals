<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta content="" name="description">
    <meta content="" name="keywords">
    <meta content="140900d73f72a2:13187910af46dc" name="leadpages-meta-id">
    <meta content="//brinker.leadpages.co" name="leadpages-url">
    <meta name="leadpages-served-by" content="html" />

    <link href="/Content/themes/landers/style.css" rel="stylesheet" />
    <link href="/Content/spinn.css" rel="stylesheet" />
    <!--<link href="/tripwire/css/style.css" rel="stylesheet" type="text/css" />-->
    <link href="https://fonts.googleapis.com/css?family=Merriweather:100,300,400italic,700%7COpen+Sans:300,400,600,700" rel="stylesheet" type="text/css">
    <!-- jQuery, Angular -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js" type="text/javascript"></script>
    <script src="/Script/landers/bootstrap.min.js"></script>
    <script src="/Script/spin.min.js"></script>
</head>
<body>
    <div>
        @RenderBody()
    </div>

    <div id="loading">
        <div id="loadingcontent" style="">
            <p id="loadingspinner">
                <strong>Please wait while we process your order.</strong>
            </p>
        </div>
    </div>
    <div id="dialog-message" title="Errors" class="modal fade" tabindex="-1" role="dialog"
         aria-labelledby="myModalLabel1" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <section class="modal-header text-left">
                    <h3 id="myModalLabel1" style="color: Red">Error !</h3>
                </section>
                <section class="modal-body modal-body-alt text-left">
                    Please address the following error:<br /><br />
                    <span id="spnErrorMessage"></span>
                </section>
                <section class="modal-footer">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="btn btn-primary">
                        Okay
                    </button>
                </section>
            </div>
        </div>
    </div>
    <div class="modal fade newModal" id="upsell1Modal" tabindex="-1" role="dialog"
         aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <section class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </section>
                <section class="modal-body">
                    <div class="col-md-10 offset1">
                        <div id="divProcessMessage" class="text-center;" style="padding: 30px;">
                            <p>
                                <span id="spnModalSuccessMessage" style="display: none;">
                                    Thank you! We’ve added your
                                    bonus selection to your order.
                                </span> <span id="spnModalFailedMessage" style="display: none;">
                                    <strong>Thank you for your interest in this Bonus Offer.</strong><br />
                                    Due to technical difficulties, we are not able to add the bonus to your order.<br />
                                    Please call us at 1-800-353-3140 to place an order via phone.<br />
                                    Thank you again for your interest and understanding.
                                </span>
                            </p>
                        </div>
                    </div>
                </section>
                <section class="modal-footer">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="btn btn-primary">
                        Okay
                    </button>
                </section>
            </div>
        </div>
    </div>
</body>
</html>
